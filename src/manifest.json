{
    "manifest_version": 2,
    "name": "app-name",
    "version": "0.1.0",
    "minimum_chrome_version": "33",
    "default_locale": "en",
    "icons": {
        "16": "img/app/16x16.png",
        "48": "img/app/48x48.png",
        "128": "img/app/128x128.png"
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "sockets": {
        "udp": {
            "send": "",
            "bind": ""
        }
    },
    "permissions": [
        "storage",
        "http://*/description.xml"
    ]
}